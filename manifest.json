{
	"manifest_version" : 2,
	"name" : "JoJ",
	"short_name" : " Notifications Twitch",
	"description" : "Etre au courant quand JoJ est en live, accéder au planning de la semaine et liens vers les réseaux sociaux",
	"version" : "1.7.8",
	"author" : "Azamir",

	"permissions" : [
		"notifications",
		"activeTab",
		"storage",
		"https://twitch.tv/*",
		"https://discordapp.com/invite/*",
		"https://twitter.com/*",
		"https://www.youtube.com/channel/*"
	],
	"browser_action" : {

		"default_icon" : "img/JoJ_radius_64.png",
        "default_popup" : "popup.html"
	},


	"icons" : {
		"64" : "img/JoJ_radius_64.png",
		"128" : "img/JoJ_radius_128.png",
		"256" : "img/JoJ_radius_256.png"
	},

	"background" : {
		"scripts" : [
			"js/app.js",
			"js/jquery.js",
			"js/config.js",
			"js/background.js"
		]
	}


}