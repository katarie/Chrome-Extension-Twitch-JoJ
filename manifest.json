{
	"manifest_version" : 2,
	"name" : "JoJ",
	"short_name" : " Notifications Twitch",
	"description" : "Etre au courant quand JoJ est en live, acc√©der au planning de la semaine et liens vers les reseaux sociaux", //TODO : changer la description
	"version" : "1.4.9",
	"author" : "Azamir",

	"permissions" : [
		"http://*/*",
		"https://*/*", 
		"notifications",
		"tabs",
		"storage"
	],
	"browser_action" : {

		"default_icon" : "img/JoJ_64.png",
        "default_popup" : "popup.html"
	},


	"icons" : {
		"64" : "img/JoJ_64.png",//TODO : changer l'icone par defaut
		"128" : "img/JoJ_transparent_128.png",
		"256" : "img/JoJ_transparent_256.png"
	},

	"background" : {
		"scripts" : [
			"js/app.js",
			"js/jquery.js",
			"js/config.js",
			"js/background.js"
		]
	}


}